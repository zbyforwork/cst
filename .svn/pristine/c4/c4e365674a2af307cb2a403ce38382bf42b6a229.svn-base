<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fresco="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f9f9f9"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/re_top"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_background_size"
        android:background="@color/appColor"
        android:focusable="true"
        android:focusableInTouchMode="true"
        >

        <ImageView
            android:id="@+id/iv_back"
            android:layout_width="@dimen/title_image_width_25"
            android:layout_height="@dimen/title_image_height_25"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:src="@drawable/back"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerInParent="true"
            android:text="车辆详情"
            android:textColor="@color/bg_White"
            android:textSize="@dimen/title_font_size"
            />
    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <com.facebook.drawee.view.SimpleDraweeView
                android:id="@+id/iv_car_image"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                fresco:placeholderImageScaleType="centerCrop"
                />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal"
                android:layout_marginTop="5dp"
                android:background="@color/bg_White"
                >
                <TextView
                    android:id="@+id/label_name"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="车辆所属区域"
                    android:gravity="right"
                    android:layout_centerVertical="true"
                    android:textColor="@color/black"
                    android:layout_marginLeft="16dp"
                    />
                <TextView
                    android:id="@+id/tv_nation_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/label_name"
                    android:layout_marginLeft="5dp"
                    android:background="@null"
                    android:selectAllOnFocus="true"
                    android:layout_centerVertical="true"
                    android:textColor="@color/font_grey_color"
                    android:textColorHint="@color/font_grey_color"
                    android:text="史小鹏"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal"
                android:background="@color/bg_White"
                >
                <TextView
                    android:id="@+id/label_idcard"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="车辆品牌"
                    android:gravity="right"
                    android:layout_centerVertical="true"
                    android:textColor="@color/black"
                    android:layout_marginLeft="16dp"
                    />
                <TextView
                    android:id="@+id/tv_brand_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/label_idcard"
                    android:layout_marginLeft="5dp"
                    android:background="@null"
                    android:selectAllOnFocus="true"
                    android:layout_centerVertical="true"
                    android:textColor="@color/font_grey_color"
                    android:textColorHint="@color/font_grey_color"
                    android:text="151566451215645"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal"
                android:background="@color/bg_White"
                >
                <TextView
                    android:id="@+id/label_tel"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="车架号后6位"
                    android:gravity="right"
                    android:layout_centerVertical="true"
                    android:textColor="@color/black"
                    android:layout_marginLeft="16dp"
                    />
                <TextView
                    android:id="@+id/tv_frame_number"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/label_tel"
                    android:layout_marginLeft="5dp"
                    android:background="@null"
                    android:selectAllOnFocus="true"
                    android:layout_centerVertical="true"
                    android:textColor="@color/font_grey_color"
                    android:textColorHint="@color/font_grey_color"
                    android:text="1484121"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal"
                android:background="@color/bg_White"
                >
                <TextView
                    android:id="@+id/label_address"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="车牌号类型"
                    android:gravity="right"
                    android:layout_centerVertical="true"
                    android:textColor="@color/black"
                    android:layout_marginLeft="16dp"
                    />
                <TextView
                    android:id="@+id/tv_platetype"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/label_address"
                    android:layout_marginLeft="5dp"
                    android:background="@null"
                    android:selectAllOnFocus="true"
                    android:layout_centerVertical="true"
                    android:textColor="@color/font_grey_color"
                    android:textColorHint="@color/font_grey_color"
                    android:text="成都市龙泉驿"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rel_carType"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="@color/bg_White">


                <TextView
                    android:id="@+id/label_carType"
                    android:layout_width="100dp"
                    android:gravity="right"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="5dp"
                    android:background="@null"
                    android:singleLine="true"
                    android:text="车牌号"
                    android:textColor="@color/black"
                    android:textSize="@dimen/normal_font_size"/>

                <TextView
                    android:id="@+id/tv_plate_number"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/label_carType"
                    android:layout_marginLeft="5dp"
                    android:background="@null"
                    android:text="奥迪A6L"
                    android:singleLine="true"
                    android:textColor="@color/font_grey_color"
                    android:textSize="@dimen/normal_font_size"/>
            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/rel_carNumber"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_below="@+id/rel_carType"
                android:background="@color/bg_White">


                <TextView
                    android:id="@+id/label_carNumber"
                    android:layout_width="100dp"
                    android:gravity="right"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="5dp"
                    android:background="@null"
                    android:singleLine="true"
                    android:text="车型名称"
                    android:textColor="@color/black"
                    android:textSize="@dimen/normal_font_size"/>
                <TextView
                    android:id="@+id/tv_model"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/label_carNumber"
                    android:layout_marginLeft="5dp"
                    android:background="@null"
                    android:text="川滴滴滴滴"
                    android:textColor="@color/font_grey_color"
                    android:textSize="@dimen/normal_font_size"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rel_chejiahao"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_below="@+id/rel_carNumber"
                android:background="@color/bg_White">


                <TextView
                    android:id="@+id/label_chejiahao"
                    android:layout_width="100dp"
                    android:gravity="right"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="5dp"
                    android:background="@null"
                    android:singleLine="true"
                    android:text="添加时间"
                    android:textColor="@color/black"
                    android:textSize="@dimen/normal_font_size"/>

                <TextView
                    android:id="@+id/tv_create_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/label_chejiahao"
                    android:layout_marginLeft="5dp"
                    android:background="@null"
                    android:text="LDLSLDL5156464"
                    android:textColor="@color/font_grey_color"
                    android:textSize="@dimen/normal_font_size"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rel_fadongjihao"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_below="@+id/rel_chejiahao"
                android:background="@color/bg_White">


                <TextView
                    android:id="@+id/label_fadongjihao"
                    android:layout_width="100dp"
                    android:gravity="right"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="5dp"
                    android:background="@null"
                    android:singleLine="true"
                    android:text="别名"
                    android:textColor="@color/black"
                    android:textSize="@dimen/normal_font_size"/>

                <TextView
                    android:id="@+id/tv_alias"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/label_fadongjihao"
                    android:layout_marginLeft="5dp"
                    android:background="@null"
                    android:selectAllOnFocus="true"
                    android:text="NJSD51156123"
                    android:textColor="@color/font_grey_color"
                    android:textSize="@dimen/normal_font_size"/>

            </RelativeLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>




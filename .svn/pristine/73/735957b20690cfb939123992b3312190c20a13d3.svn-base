package com.hxkj.cst.cheshuotong.utils;

import android.util.Log;

public class MyLog {
    public static boolean isLog = true;

    public static void i(String text) {
        Log.i("yaochuan", text);
    }

    public static void e(String errorText) {
        if (isLog) {
            Log.e("james", errorText);
        }
    }

    public static void e(String errorText, Throwable throwable) {
        if (isLog) {
            Log.e("james", errorText, throwable);
        }
    }

}

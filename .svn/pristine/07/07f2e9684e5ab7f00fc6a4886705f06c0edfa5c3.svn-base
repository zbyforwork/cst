package com.hxkj.cst.cheshuotong.bean;

/**
 * 车辆品牌
 * Created by dell on 2015/11/11.
 */
public class CLPP {

    /**
     * PPID : 品牌id
     * PPMC : 品牌名称
     * PPTB : 品牌图标
     * PPSZM : 品牌首字母
     */

    private String PPID;
    private String PPMC;
    private String PPTB;
    private String PPSZM;

    public void setPPID(String PPID) {
        this.PPID = PPID;
    }

    public void setPPMC(String PPMC) {
        this.PPMC = PPMC;
    }

    public void setPPTB(String PPTB) {
        this.PPTB = PPTB;
    }

    public void setPPSZM(String PPSZM) {
        this.PPSZM = PPSZM;
    }

    public String getPPID() {
        return PPID;
    }

    public String getPPMC() {
        return PPMC;
    }

    public String getPPTB() {
        return PPTB;
    }

    public String getPPSZM() {
        return PPSZM;
    }
}
